# Frontend Environment Variables (Vercel)
# Copie este arquivo para .env.local para desenvolvimento local
# Configure estas variáveis no Dashboard do Vercel para produção

# API Configuration
VITE_API_URL=http://localhost:3001

# Appwrite Configuration
VITE_APPWRITE_ENDPOINT=https://cloud.appwrite.io/v1
VITE_APPWRITE_PROJECT_ID=your-project-id-here
VITE_APPWRITE_DATABASE_ID=horizon_ai_db

# Appwrite Collections
VITE_APPWRITE_COLLECTION_ACCOUNTS=accounts
VITE_APPWRITE_COLLECTION_TRANSACTIONS=transactions
VITE_APPWRITE_COLLECTION_CREDIT_CARDS=credit_cards

# Google AI (Optional)
VITE_GOOGLE_AI_API_KEY=your_google_ai_key_here
